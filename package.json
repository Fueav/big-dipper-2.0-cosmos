{
  "name": "big-dipper-2.0-cosmos",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn run build --concurrenc 1",
    "clean": "turbo run clean --parallel --continue",
    "dev": "turbo run dev --no-cache --continue",
    "dev:agoric": "turbo run dev --no-cache --filter web-agoric",
    "dev:akash": "turbo run dev --no-cache --filter web-akash",
    "dev:assetmantle": "turbo run dev --no-cache --filter web-assetmantle",
    "dev:band": "turbo run dev --no-cache --filter web-band",
    "dev:bitsong": "turbo run dev --no-cache --filter web-bitsong",
    "dev:comdex": "turbo run dev --no-cache --filter web-comdex",
    "dev:cosmos": "turbo run dev --no-cache --filter web-cosmos",
    "dev:crescent": "turbo run dev --no-cache --filter web-crescent",
    "dev:desmos": "turbo run dev --no-cache --filter web-desmos",
    "dev:elrond": "turbo run dev --no-cache --filter web-elrond",
    "dev:emoney": "turbo run dev --no-cache --filter web-emoney",
    "dev:evmos": "turbo run dev --no-cache --filter web-evmos",
    "dev:flow": "turbo run dev --no-cache --filter web-flow",
    "dev:likecoin": "turbo run dev --no-cache --filter web-likecoin",
    "dev:nomic": "turbo run dev --no-cache --filter web-nomic",
    "dev:nym": "turbo run dev --no-cache --filter web-nym",
    "dev:osmosis": "turbo run dev --no-cache --filter web-osmosis",
    "dev:persistence": "turbo run dev --no-cache --filter web-persistence",
    "dev:provenance": "turbo run dev --no-cache --filter web-provenance",
    "dev:regen": "turbo run dev --no-cache --filter web-regen",
    "dev:rizon": "turbo run dev --no-cache --filter web-rizon",
    "dev:shentu": "turbo run dev --no-cache --filter web-shentu",
    "dev:sifchain": "turbo run dev --no-cache --filter web-sifchain",
    "dev:solana": "turbo run dev --no-cache --filter web-solana",
    "dev:stride": "turbo run dev --no-cache --filter web-stride",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "test": "turbo run test --parallel",
    "e2e": "turbo run e2e --parallel",
    "graphql:codegen": "turbo run graphql:codegen --parallel"
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0"
  },
  "devDependencies": {
    "prettier": "2",
    "turbo": "latest"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">=16.13.0",
    "yarn": ">=3.2.4"
  }
}
